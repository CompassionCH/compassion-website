<odoo>
    <template
    id="assets_crowdfunding"
    name="Assets for crowdfunding"
    inherit_id="website.assets_frontend"
  >
        <xpath expr="//script[last()]" position="after">
            <script
        src="/crowdfunding_compassion/static/src/js/search.js"
        type="text/javascript"
      />
            <script
        src="/crowdfunding_compassion/static/src/js/website_widgets.js"
        type="text/javascript"
      />
        </xpath>
    </template>
    <template id="assets_editor" inherit_id="website.assets_editor">
        <xpath expr="." position="inside">
            <script
        type="text/javascript"
        src="/crowdfunding_compassion/static/src/js/form_snippet.js"
      />
        </xpath>
    </template>
</odoo>
