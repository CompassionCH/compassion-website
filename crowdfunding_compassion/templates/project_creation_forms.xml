                  >
                                        <div class="card border-0">
                                            <a
                        t-attf-id="product-choose-#{fund_index}"
                        class="btn text-center"
                      >
                                                <img
                          t-attf-src="data:image;base64,#{fund.image_128}"
                          class="d-block mx-auto"
                          t-if="fund.image_128"
                          width="128px"
                        />
