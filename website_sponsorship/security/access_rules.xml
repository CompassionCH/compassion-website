      name="name"
    >Compassion Child : public access for sponsoring</field>
        <field name="model_id" ref="model_compassion_child" />
        <field name="domain_force">[('state', 'in', ['N', 'I'])]</field>
        <field name="groups" eval="[(4, ref('base.group_public'))]" />
